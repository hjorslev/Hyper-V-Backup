    <command:details>
      <command:name>Backup-VM</command:name>
      <command:verb>Backup</command:verb>
      <command:noun>VM</command:noun>
      <maml:description>
        <maml:para>Perform a backup of Hyper-V Virtual Machines.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Perform a backup of Hyper-V Virtual Machines. This module exports Virtual Machines and compress them using 7zip.</maml:para>
    </maml:description>
